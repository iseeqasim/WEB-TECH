  <div class="containers">
    <div class="containerr mt-5">
      <div class="row justify-content-center">
        <div class="col-md-6">
          <div class="form-group">
            <select class="form-control" id="movieSelect">
              <option value="">Select Movie</option>
              <option value="Avatar: The Way of Water">Avatar: The Way of Water</option>
              <option value="All Quiet on the Western Front">All Quiet on the Western Front</option>
              <option value="Puss in Boots">Puss in Boots</option>
