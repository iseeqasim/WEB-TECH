    <script>
      var movieSelect = document.getElementById("movieSelect");
      movieSelect.addEventListener("change", function() {
        var selectedMovie = movieSelect.options[movieSelect.selectedIndex].value;
        alert("You selected: " + selectedMovie);
      });
    </script>

  
    
